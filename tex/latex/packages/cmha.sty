%defined values for /myperson
%human lengths between joints
\newcommand*{\torsoL}{4.9}%
\newcommand*{\armL}{2.7}
\newcommand*{\forearmL}{2.7}
\newcommand*{\thighL}{4.5}
\newcommand*{\calfL}{4}

%ellipses lengthened by this to enclose joints
\newcommand*{\jointscale}{1.18}


\newcommand{\myperson}[1][]{
	\pgfkeys{/myperson, default, #1}

\coordinate (P) at (0+\mypersonpelvisX,0+\mypersonpelvisY); %pelvis
\coordinate (S) at ($ (P) + (\mypersontorsoA:\torsoL) $); %shoulder
\coordinate (E) at ($ (S) + (\mypersonarmA:\armL)$); %elbow
\coordinate (W) at ($ (E) + (\mypersonforearmA:\forearmL)$); %wrist

\coordinate (Ka) at ($ (P) + (\mypersonthighaA:\thighL) $);%knee a
\coordinate (Kb) at ($ (P) + (\mypersonthighbA:\thighL) $);%knee b
\coordinate (Aa) at ($ (Ka) + (\mypersoncalfaA:\calfL)$);%ankle a
\coordinate (Ab) at ($ (Kb) + (\mypersoncalfbA:\calfL)$);%ankle b

\coordinate (H) at ($ {-1}*(P) + {1+0.33}*(S) - {1+0.33}*(P) $); %head

\coordinate (T) at ($ 0.5*(P) + 0.5*(S) $);
\coordinate (A) at ($ 0.5*(S) + 0.5*(E) $);
\coordinate (F) at ($ 0.5*(E) + 0.5*(W) $);
\coordinate (Ta) at ($ 0.5*(P) + 0.5*(Ka) $);
\coordinate (Tb) at ($ 0.5*(P) + 0.5*(Kb) $);
\coordinate (Ca) at ($ 0.5*(Ka) + 0.5*(Aa) $);
\coordinate (Cb) at ($ 0.5*(Kb) + 0.5*(Ab) $);
\coordinate (T) at ($ 0.5*(P) + 0.5*(S) $);

\ellip{(S)}{0}{.3}{.3}
\ellip{(E)}{0}{.2}{.2}
\ellip{(W)}{0}{.2}{.2}

\ellip{(P)}{0}{.2}{.2}
\ellip{(Ka)}{0}{.2}{.2}
\ellip{(Kb)}{0}{.2}{.2}
\ellip{(Aa)}{0}{.2}{.2}
\ellip{(Ab)}{0}{.2}{.2}
\ellip{(P)}{0}{.2}{.2}

\ellip{(T)}{{\mypersontorsoA}}{\jointscale*\torsoL/2}{1.1}
\ellip{(A)}{{\mypersonarmA}}{\jointscale*\armL/2}{.5}
\ellip{(F)}{{\mypersonforearmA}}{\jointscale*\forearmL/2}{.4}
\ellip{(Ta)}{{\mypersonthighaA}}{\jointscale*\thighL/2}{.8}
\ellip{(Tb)}{{\mypersonthighbA}}{\jointscale*\thighL/2}{.8}
\ellip{(Ca)}{{\mypersoncalfaA}}{\jointscale*\calfL/2}{.6}
\ellip{(Cb)}{{\mypersoncalfbA}}{\jointscale*\calfL/2}{.6}
\ellip{(H)}{\mypersontorsoA}{1.3}{.9}

}



